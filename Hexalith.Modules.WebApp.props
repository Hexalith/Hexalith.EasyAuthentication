<Project>
  <PropertyGroup>
    <HexalithModulesClientProps>$([MSBuild]::GetPathOfFileAbove('Hexalith.Modules.WebApp.props', '$(MSBuildThisFileDirectory)..'))</HexalithModulesClientProps>
  </PropertyGroup>
  <Import Project="$(HexalithModulesClientProps)" Condition=" '$(HexalithModulesClientProps)' != '' " />
  <ItemGroup Condition="'$(UseProjectReference)' == 'true'">
    <ProjectReference Include="$(HexalithSecurityPath)src\Modules\Hexalith.Security.SharedAssets\Hexalith.Security.SharedAssets.csproj" />
    <ProjectReference Include="$(HexalithSecurityPath)src\Modules\Hexalith.Security.WebApp\Hexalith.Security.WebApp.csproj" />
    <ProjectReference Include="$(HexalithSecurityPath)src\Presentation\Hexalith.Security.UI.Components\Hexalith.Security.UI.Components.csproj" />
    <ProjectReference Include="$(HexalithSecurityPath)src\Presentation\Hexalith.Security.UI.Pages\Hexalith.Security.UI.Pages.csproj" />
  </ItemGroup>
  <ItemGroup Condition="'$(UseProjectReference)' != 'true'">
    <PackageReference Include="Hexalith.Security.SharedAssets" Version="$(HexalithSecurityVersion)" />
    <PackageReference Include="Hexalith.Security.WebApp" Version="$(HexalithSecurityVersion)" />
    <PackageReference Include="Security.UI.Components" Version="$(HexalithSecurityVersion)" />
    <PackageReference Include="Hexalith.Security.UI.Pages" Version="$(HexalithSecurityVersion)" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="$(MSBuildThisFileDirectory)src/WebAppApplication.cs" />
  </ItemGroup>
</Project>